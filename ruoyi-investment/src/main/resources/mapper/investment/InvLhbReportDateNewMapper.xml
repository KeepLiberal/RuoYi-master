<mapper namespace="com.ruoyi.investment.mapper.InvLhbReportDateNewMapper">
    
    <resultMap type="InvLhbReportDateNew" id="InvLhbReportDateNewResult">
        <result property="id"    column="id"    />
        <result property="securityCode"    column="security_code"    />
        <result property="tradeDate"    column="trade_date"    />
    </resultMap>

    <sql id="selectInvLhbReportDateNewVo">
        select id, security_code, trade_date from inv_lhb_report_date_new
    </sql>

    <select id="selectInvLhbReportDateNewList" parameterType="InvLhbReportDateNew" resultMap="InvLhbReportDateNewResult">
        <include refid="selectInvLhbReportDateNewVo"/>
        <where>  
            <if test="securityCode != null  and securityCode != ''"> and security_code = #{securityCode}</if>
            <if test="tradeDate != null "> and trade_date = #{tradeDate}</if>
        </where>
    </select>
    
    <select id="selectInvLhbReportDateNewById" parameterType="Integer" resultMap="InvLhbReportDateNewResult">
        <include refid="selectInvLhbReportDateNewVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertInvLhbReportDateNew" parameterType="InvLhbReportDateNew" useGeneratedKeys="true" keyProperty="id">
        insert into inv_lhb_report_date_new
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="securityCode != null and securityCode != ''">security_code,</if>
            <if test="tradeDate != null">trade_date,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="securityCode != null and securityCode != ''">#{securityCode},</if>
            <if test="tradeDate != null">#{tradeDate},</if>
         </trim>
    </insert>

    <update id="updateInvLhbReportDateNew" parameterType="InvLhbReportDateNew">
        update inv_lhb_report_date_new
        <trim prefix="SET" suffixOverrides=",">
            <if test="securityCode != null and securityCode != ''">security_code = #{securityCode},</if>
            <if test="tradeDate != null">trade_date = #{tradeDate},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteInvLhbReportDateNewById" parameterType="Integer">
        delete from inv_lhb_report_date_new where id = #{id}
    </delete>

    <delete id="deleteInvLhbReportDateNewByIds" parameterType="String">
        delete from inv_lhb_report_date_new where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <delete id="deleteInvLhbReportDateNewAll">
        delete from inv_lhb_report_date_new
    </delete>

</mapper>